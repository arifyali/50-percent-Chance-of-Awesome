eval_closure = function(t) {
f = function(x) {return(t*(3*x[1]^2-2*x[1]*x[2]+3*x[2]^2)-(log(1-x[1])+log(x[1])+log(1-x[2])+log(x[2])))}
}
gradient_closure = function(t) {
f = function(x) {
dx1 = t*(6*x[1]-2*x[2])+1/(1-x[1])-1/x[1]
dx2 = t*(6*x[2]-2*x[1])+1/(1-x[2])-1/x[2]
return(c(dx1,dx2))
}
return(f)
}
hessian_closure = function(t) {
f = function(x) {
dx1x1 = 6*t+1/(1-x[1])^2+1/x[1]^2
dx1x2 = -2*t
dx2x1 = -2*t
dx2x2 = 6*t+1/(1-x[2])^2+1/x[2]^2
return(c(dx1x1,dx1x2,dx2x1,dx2x2))
}
return(f)
}
backtracking = function(f,x0,df0,dx,a,c){
t = 1
f0 = f(x0)
delta = t(df0)%*%dx
while (f(x0+t*dx) > f0+a*t*delta){
t = t*c
}
return (x0+t*dx)
}
f = eval_closure(1)
df = gradient_closure(1)
d2f = hessian_closure(1)
b = c(1/2,1/2)
x = data.frame(matrix(NA, nrow = 100, ncol = 2))
for(i in 1:15){
b_new = backtracking(f,b,df(b),solve(matrix(d2f(b),2),matrix(-df(b))),0.25,0.5)
b = b_new
print(b)
}
x[1,] = b
for (t in 2:100){
f = eval_closure(t)
df = gradient_closure(t)
d2f = hessian_closure(t)
for (i in 1:5){
